{
    "name": "rapidez/riverty",
    "description": "Rapidez Riverty",
    "keywords": [
        "rapidez",
        "riverty",
        "afterpay"
    ],
    "homepage": "https://rapidez.io",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "indy koning",
            "email": "indy@justbetter.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "rapidez/core": "^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rapidez\\Riverty\\RivertyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Rapidez\\Riverty\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
